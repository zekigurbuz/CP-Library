int p0w(int n, int p, int m) {
  if (p == 0) return 1;
  if (p == 1) return n % m;
  if (p % 2 == 0) {
    int t = p0w(n, p / 2, m);
    return t % m * t % m % m;
  }
  int t = p0w(n, p / 2, m);
  t = t % m * t % m % m;
  return t % m * n % m % m;
}
int p0w(int n, int p) {
  if (p == 0) return 1;
  if (p == 1) return n;
  if (p % 2 == 0) {
    int t = p0w(n, p / 2);
    return t * t;
  }
  int t = p0w(n, p / 2);
  return t * t * n;
}
